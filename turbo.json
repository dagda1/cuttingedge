{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*"]
    },
    "test": {
      "dependsOn": [],
      "outputs": ["coverage/**/*"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "src/test/**/*.ts", "src/test/**/*.tsx"]
    },
		"lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    }
  }
}