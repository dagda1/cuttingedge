{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "tsconfig-base.json",
    "tsconfig.packages.json"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*"]
    },
    "test": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "src/test/**/*.ts", "src/test/**/*.tsx"]
    },
		"lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    }
  }
}