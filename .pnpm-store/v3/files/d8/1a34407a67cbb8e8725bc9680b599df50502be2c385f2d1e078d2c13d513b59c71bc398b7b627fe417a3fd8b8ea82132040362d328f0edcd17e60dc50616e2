import { createFuture } from '../future';
export function createSuspendController() {
    let { produce, future } = createFuture();
    function start() {
        // no op
    }
    function halt() {
        produce({ state: 'halted' });
    }
    return { start, halt, future, type: 'suspend', operation: undefined };
}
