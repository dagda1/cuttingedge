import type { CapsizeOptions, ComputedValues } from './types';
export declare function createStyleObject(args: CapsizeOptions | ComputedValues): {
    fontSize: string;
    lineHeight: string;
    '::before': {
        content: string;
        marginBottom: string;
        display: string;
    };
    '::after': {
        content: string;
        marginTop: string;
        display: string;
    };
};
