/**
 * Receives two dates to compare and returns "time ago" based on them
 * example: 4 weeks ago
 *
 * Heavily inspired by https://stackoverflow.com/a/67338038/938822
 */
export declare const fromNow: (date: Date | number | string, nowDate?: Date | number | string, rft?: Intl.RelativeTimeFormat) => string | undefined;
