{
  "name": "@cutting/react-abortable-fetch",
  "description": "A simpler use-fetch or use-query or whatever.",
  "version": "0.28.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge/tree/main/packages/react-abortable-fetch"
  },
  "main": "dist/cjs/index.js",
  "browser": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "start": "PORT=8888 NODE_ENV=development devtools devserver-start",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test devtools test",
    "build": "NODE_ENV=production devtools rollup"
  },
  "dependencies": {
    "@cutting/util": "workspace:*",
    "@effection/channel": "2.0.3",
    "@effection/core": "2.2.0",
    "@effection/events": "2.0.3",
    "@effection/fetch": "2.0.4",
    "@effection/main": "2.0.3",
    "@effection/subscription": "2.0.3",
    "@effection/stream": "2.0.3",
    "@xstate/react": "^3.0.0",
    "abortcontroller-polyfill": "^1.7.3",
    "assert-ts": "^0.3.4",
    "cross-fetch": "^3.1.5",
    "effection": "^2.0.4",
    "fetch-jsonp": "^1.2.1",
    "xstate": "^4.31.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.18.3",
    "@cutting/devtools": "workspace:*",
    "@cutting/eslint-config": "workspace:*",
    "@cutting/tsconfig": "workspace:*",
    "@cutting/useful-types": "workspace:*",
    "@testing-library/react-hooks": "8.0.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@vanilla-extract/babel-plugin": "1.1.6",
    "@vanilla-extract/css": "^1.7.1",
    "classnames": "2.3.1",
    "eslint": "8.17.0",
    "msw": "0.39.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "typescript": "4.7.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">= 18.x.x",
    "react-dom": ">= 18.x.x"
  },
  "keywords": [
    "hotkeys",
    "focus",
    "react",
    "reactjs",
    "reactnative",
    "hooks",
    "hook",
    "use-cancellable-fetch"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "msw": {
    "workerDirectory": "demo/public"
  },
  "lint-staged": {
    "*.js": "eslint ./src/**/*.{js} --fix",
    "*.ts": "eslint ./src/**/*.{js} --fix"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "browser": "./dist/umd/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  }
}
