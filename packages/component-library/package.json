{
  "name": "@cutting/component-library",
  "version": "5.4.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "main": "dist/cjs/index.js",
  "sideEffects": [
    "./src/style/reset/*",
    "**/*.css.ts",
    "*.css"
  ],
  "types": "dist/esm/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cutting/hooks": "workspace:*",
    "@cutting/util": "workspace:*",
    "assert-ts": "^0.3.4",
    "classnames": "^2.3.1",
    "deepmerge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.2",
    "@capsizecss/core": "^3.0.0",
    "@capsizecss/vanilla-extract": "^1.0.0",
    "@cutting/devtools": "workspace:*",
    "@cutting/eslint-config": "workspace:*",
    "@cutting/tsconfig": "workspace:*",
    "@cutting/useful-types": "workspace:*",
    "@mdx-js/react": "^2.0.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/babel__core": "^7",
    "@types/dedent": "^0.7.0",
    "@types/mdx-js__react": "^1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.8",
    "@vanilla-extract/babel-plugin": "^1.1.4",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/sprinkles": "^1.3.3",
    "autoprefixer": "^10.4.2",
    "csstype": "^3.0.10",
    "dedent": "^0.7.0",
    "esbuild": "^0.14.21",
    "eslint": "^8.9.0",
    "node-fetch": "^3.2.0",
    "polished": "^4.1.4",
    "postcss": "^8.4.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "serve-handler": "^6.1.3",
    "storybook-addon-designs": "^6.2.1",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0"
  },
  "peerDependencies": {
    "react": ">= 17.x.x",
    "react-dom": ">= 17.x.x",
    "react-router": ">= 6.x.x"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "prebuild": "tsc",
    "build": "NODE_ENV=production cutting esbuild && tsc --emitDeclarationOnly -p ./tsconfig.dist.json",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "start": "PORT=8888 NODE_ENV=development cutting devserver-start",
    "test": "NODE_ENV=test cutting test",
    "test:ci": "CI=true pnpm test",
    "prepack": "pnpm build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/umd/index.js"
    },
    "./styles.css": {
      "import": "./dist/esm/index.css",
      "require": "./dist/esm/index.css",
      "browser": "./dist/esm/index.css"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  }
}
