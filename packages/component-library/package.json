    "eslint": "8.42.0",
    "node-fetch": "3.3.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.11.2",
    "require-from-string": "^2.0.2",
