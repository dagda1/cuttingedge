{
  "name": "@cutting/component-library",
  "version": "5.24.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "main": "dist/cjs/index.js",
  "sideEffects": [
    "*.css"
  ],
  "types": "dist/esm/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cutting/hooks": "workspace:*",
    "@cutting/util": "workspace:*",
    "assert-ts": "^0.3.4",
    "classnames": "^2.3.1",
    "deepmerge": "^4.2.2",
    "polished": "4.2.2"
  },
  "devDependencies": {
    "@babel/runtime": "7.17.9",
    "@capsizecss/core": "3.0.0",
    "@capsizecss/vanilla-extract": "1.0.0",
    "@cutting/devtools": "workspace:*",
    "@cutting/eslint-config": "workspace:*",
    "@cutting/tsconfig": "workspace:*",
    "@cutting/useful-types": "workspace:*",
    "@mdx-js/react": "2.1.1",
    "@storybook/addon-actions": "6.4.21",
    "@storybook/addon-docs": "6.4.21",
    "@storybook/addon-essentials": "6.4.21",
    "@storybook/addon-links": "6.4.21",
    "@storybook/builder-webpack5": "6.4.21",
    "@storybook/manager-webpack5": "6.4.21",
    "@storybook/react": "6.4.21",
    "@testing-library/react": "13.0.0",
    "@types/babel__core": "7.1.19",
    "@types/dedent": "0.7.0",
    "@types/mdx-js__react": "1.5.5",
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "@types/tailwindcss": "3.0.10",
    "@vanilla-extract/babel-plugin": "1.1.5",
    "@vanilla-extract/css": "1.6.8",
    "@vanilla-extract/sprinkles": "1.4.0",
    "autoprefixer": "10.4.4",
    "csstype": "3.0.11",
    "dedent": "0.7.0",
    "esbuild": "0.14.34",
    "eslint": "8.13.0",
    "node-fetch": "3.2.3",
    "postcss": "8.4.12",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-router": "6.3.0",
    "serve-handler": "6.1.3",
    "storybook-addon-designs": "6.2.1",
    "tailwindcss": "3.0.23",
    "typescript": "4.6.3",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "react": ">= 18.x.x",
    "react-dom": ">= 18.x.x",
    "react-router": ">= 6.x.x"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "prebuild": "tsc",
    "build": "NODE_ENV=production devtools esbuild && tsc --emitDeclarationOnly -p ./tsconfig.dist.json",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "start": "PORT=8888 NODE_ENV=development devtools devserver-start",
    "test": "NODE_ENV=test devtools test",
    "test:ci": "CI=true pnpm test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/umd/index.js"
    },
    "./styles.css": {
      "import": "./dist/esm/index.css",
      "require": "./dist/esm/index.css",
      "browser": "./dist/esm/index.css"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  },
  "pnpm": {
    "overrides": {
      "@babel/core@<7.17.8": ">=7.17.8",
      "@babel/types@<7.17.0": ">=7.17.3",
      "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
      "acorn@<8.7.0": ">=8.7.0"
    }
  }
}
