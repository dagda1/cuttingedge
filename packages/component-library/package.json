{
  "name": "@cutting/component-library",
  "version": "5.4.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "main": "dist/cjs/index.js",
  "sideEffects": [
    "./src/style/reset/*",
    "**/*.css.ts"
  ],
  "types": "dist/esm/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cutting/hooks": "4.30.4",
    "@cutting/util": "4.29.4",
    "assert-ts": "^0.3.4",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@cutting/devtools": "4.31.4",
    "@cutting/eslint-config": "4.28.2",
    "@cutting/tsconfig": "4.26.1",
    "@cutting/useful-types": "4.25.6",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/babel__core": "^7",
    "@types/dedent": "^0.7.0",
    "@types/mdx-js__react": "^1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.0",
    "babel-loader": "^8.2.3",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve-handler": "^6.1.3",
    "storybook-addon-designs": "^6.2.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": ">= 17.x.x",
    "react-dom": ">= 17.x.x"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "prebuild": "tsc",
    "build": "NODE_ENV=production cutting esbuild && tsc --emitDeclarationOnly -p ./tsconfig.dist.json",
    "lint": "yarn eslint ./src/**/*.{ts,tsx} --fix",
    "start": "PORT=8888 NODE_ENV=development cutting devserver-start",
    "test": "NODE_ENV=test cutting test",
    "test:ci": "CI=true yarn test",
    "prepack": "yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/umd/index.js"
    },
    "./styles.css": {
      "import": "./dist/esm/index.css",
      "require": "./dist/esm/index.css",
      "browser": "./dist/esm/index.css"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  }
}
