{
  "name": "@cutting/component-library",
  "version": "5.3.9",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "main": "dist/cjs/index.js",
  "sideEffects": [
    "./src/style/reset/*",
    "**/*.css.ts"
  ],
  "types": "dist/esm/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cutting/hooks": "4.29.2",
    "@cutting/util": "4.28.2",
    "assert-ts": "^0.3.2",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@cutting/devtools": "4.30.3",
    "@cutting/eslint-config": "4.27.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@types/dedent": "^0.7.0",
    "@types/postcss-normalize": "^9.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/tailwindcss": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve-handler": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "prebuild": "tsc",
    "build": "NODE_ENV=production cutting esbuild && tsc --emitDeclarationOnly -p ./tsconfig.dist.json",
    "start": "PORT=8888 NODE_ENV=development cutting devserver-start",
    "test": "NODE_ENV=test cutting test",
    "test:ci": "CI=true yarn test",
    "prepublishOnly": "yarn build"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/umd/index.js"
    },
    "./styles.css": {
      "import": "./dist/esm/index.css",
      "require": "./dist/esm/index.css",
      "browser": "./dist/esm/index.css"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  }
}
