{
  "name": "@cutting/component-library",
  "version": "5.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "module": "dist/esm/component-library.esm.js",
  "browser": "dist/umd/component-library.umd.js",
  "main": "dist/cjs/component-library.cjs.js",
  "sideEffects": [
    "./src/style/reset/*",
    "**/*.css.ts"
  ],
  "types": "dist/esm/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cutting/hooks": "4.29.1",
    "@cutting/util": "4.28.1",
    "@vanilla-extract/babel-plugin": "^1.1.1",
    "@vanilla-extract/css": "^1.6.1",
    "@vanilla-extract/sprinkles": "^1.3.0",
    "@vanilla-extract/webpack-plugin": "^2.1.0",
    "assert-ts": "^0.3.2",
    "capsize": "^2.0.0",
    "classnames": "^2.3.1",
    "csstype": "^3.0.8",
    "dedent": "^0.7.0",
    "deepmerge": "^4.2.2",
    "gradient-parser": "^1.0.2",
    "node-self": "^1.0.0-c",
    "polished": "^4.1.3",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@cutting/devtools": "4.30.2",
    "@cutting/eslint-config": "4.26.0",
    "@cutting/useful-types": "4.25.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@types/dedent": "^0.7.0",
    "@types/postcss-normalize": "^9.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/tailwindcss": "^2.2.1",
    "postcss-scss": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve-handler": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "prebuild": "tsc",
    "build": "NODE_ENV=production cutting esbuild && tsc --emitDeclarationOnly -p ./tsconfig.dist.json",
    "start": "PORT=8888 NODE_ENV=development cutting devserver-start",
    "test": "NODE_ENV=test cutting test",
    "test:ci": "CI=true yarn test",
    "prepublishOnly": "yarn build"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/component-library.esm.js",
      "require": "./dist/cjs/component-library.cjs.js",
      "browser": "./dist/umd/component-library.umd.js"
    },
    "./styles.css": {
      "import": "./dist/esm/component-library.esm.css",
      "require": "./dist/esm/component-library.esm.css",
      "browser": "./dist/esm/component-library.esm.css"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  }
}