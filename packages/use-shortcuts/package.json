{
  "name": "@cutting/use-shortcuts",
  "description": "Effortlessly create keyboard shortcuts with this react hook.",
  "version": "4.28.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "start": "PORT=8888 NODE_ENV=development cutting devserver-start",
    "test:types": "dtslint types --localTs ../../node_modules/typescript/lib --expectOnly",
    "test:unit": "NODE_ENV=test cutting test a",
    "test": "CI=true pnpm test:unit && pnpm test:types",
    "build": "NODE_ENV=production cutting rollup",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@cutting/util": "4.29.6",
    "classnames": "^2.3.1",
    "mousetrap": "^1.6.5"
  },
  "devDependencies": {
    "@cutting/devtools": "4.31.6",
    "@cutting/eslint-config": "4.28.3",
    "@cutting/tsconfig": "4.26.2",
    "@cutting/useful-types": "4.25.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/mousetrap": "^1.6.7",
    "dtslint": "^4.0.9",
    "eslint": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": ">= 17.x.x",
    "react-dom": ">= 17.x.x"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "keywords": [
    "hotkeys",
    "focus",
    "react",
    "reactjs",
    "reactnative",
    "hooks",
    "hook",
    "use-shortcuts"
  ],
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "browser": "./dist/umd/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  }
}
