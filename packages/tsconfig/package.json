{
  "name": "@cutting/tsconfig",
  "version": "4.37.1",
  "description": "Cutting Typescript Config",
  "author": "Paul Cowan <engineering@frontside.com>",
  "license": "MIT",
  "files": [
    "tsconfig.json",
    "tsconfig.test.json"
  ],
  "devDependencies": {
    "@types/webpack-env": "1.18.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "lint-staged": {
    "*.js": "eslint ./src/**/*.{js} --fix",
    "*.ts": "eslint ./src/**/*.{ts} --fix",
    "*.tsx": "eslint ./src/**/*.{tsx} --fix"
  }
}
