{
  "name": "@cutting/graphql-explorer",
  "private": true,
  "description": "Swagger for graphql.....init",
  "version": "4.37.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "start": "PORT=8888 NODE_ENV=development devtools devserver-start",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "test": "NODE_ENV=test devtools test",
    "build": "NODE_ENV=production devtools rollup"
  },
  "dependencies": {
    "@cutting/component-library": "workspace:*",
    "@cutting/react-abortable-fetch": "workspace:*",
    "@cutting/svg": "workspace:*",
    "@cutting/util": "workspace:*",
    "assert-ts": "^0.3.4",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@cutting/devtools": "workspace:*",
    "@cutting/eslint-config": "workspace:*",
    "@cutting/tsconfig": "workspace:*",
    "@cutting/useful-types": "workspace:*",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@vanilla-extract/babel-plugin": "1.1.7",
    "@vanilla-extract/css": "^1.7.4",
    "@vanilla-extract/sprinkles": "1.4.1",
    "abortcontroller-polyfill": "1.7.3",
    "eslint": "8.23.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependencies": {
    "react": ">= 18.x.x",
    "react-dom": ">= 18.x.x"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "lint-staged": {
    "*.js": "eslint ./src/**/*.{js} --fix",
    "*.ts": "eslint ./src/**/*.{ts} --fix",
    "*.tsx": "eslint ./src/**/*.{ts} --fix"
  },
  "keywords": [
    "hotkeys",
    "focus",
    "react",
    "reactjs",
    "graphql",
    "dataviz"
  ],
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "browser": "./dist/umd/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/index.d.ts"
      ]
    }
  }
}
