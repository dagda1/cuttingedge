    "assert-ts": "^0.3.4",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@cutting/devtools": "workspace:*",
    "@cutting/eslint-config": "workspace:*",
    "@cutting/tsconfig": "workspace:*",
    "@cutting/useful-types": "workspace:*",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@vanilla-extract/css": "^1.14.0",
    "@vanilla-extract/sprinkles": "1.6.1",
