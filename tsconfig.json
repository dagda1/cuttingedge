{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist/server",
    "baseUrl": ".",
    "experimentalDecorators": true,
    "paths": {
      "*": ["node_modules/*", "src/*", "config/*"]
    }
  },
  "include": ["./src/**/*"]
}
