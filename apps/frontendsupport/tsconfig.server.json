{
  "include": [ "other/**/*.mts"],
  "exclude": [],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "outDir": "./other",
    "module": "nodenext",
    "target": "ES2022",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "skipLibCheck": true
  }
}
