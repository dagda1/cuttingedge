{
  "extends": "@cutting/tsconfig",
  "compilerOptions": {
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "jsx": "preserve",
    "incremental": true,
    "types": [
      "node",
      "webpack-env",
      "@cutting/useful-types",
      "gtag.js"
    ],
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
