{
  "name": "@cutting/website",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "version": "4.29.7",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && rimraf dist && tsc -p ./tsconfig.dist.json && pnpm build:client && pnpm build:server && pnpm copy-files && ./fix-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "clean": "rimraf dist/ && mkdir dist",
    "copy-files": "copyfiles static/* dist/assets && copyfiles index.html dist && copyfiles -f dist/client/assets/* dist/assets copyfiles node_modules/@cutting/component-library/dist/esm/src.css* dist/assets",
    "start": "PORT=3000 nodemon --watch server.ts --watch src/server --exec 'ts-node --esm server.ts'",
    "lint": "eslint ./**/*.{ts,tsx} --fix",
    "serve": "NODE_ENV=production node dist/server.js",
    "deploy:web": "NODE_ENV=production pnpm build && ./bin/deploy.sh",
    "test": "NODE_ENV=test vitest"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.cs.ts"
  ],
  "dependencies": {
    "@cutting/component-library": "workspace:*",
    "@cutting/hooks": "workspace:*",
    "@cutting/svg": "workspace:*",
    "@cutting/use-get-parent-size": "workspace:*",
    "@cutting/use-mathjax": "workspace:*",
    "@cutting/util": "workspace:*",
    "@loadable/component": "5.15.2",
    "@vanilla-extract/css": "1.9.1",
    "@vanilla-extract/sprinkles": "1.5.0",
    "@visx/axis": "2.14.0",
    "@visx/curve": "2.1.0",
    "@visx/group": "2.10.0",
    "@visx/scale": "2.2.2",
    "@visx/shape": "2.12.2",
    "@visx/text": "2.12.2",
    "assert-ts": "0.3.4",
    "classnames": "2.3.2",
    "cookie-parser": "1.4.6",
    "compression": "1.7.4",
    "core-js": "3.25.5",
    "csurf": "1.11.0",
    "d3-array": "3.2.0",
    "d3-scale": "4.0.2",
    "d3-selection": "^3.0.0",
    "dayjs": "1.11.5",
    "express": "4.18.1",
    "global": "4.4.0",
    "helmet": "6.0.0",
    "history": "5.3.0",
    "mathjs": "11.3.2",
    "nocache": "3.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-router": "6.4.2",
    "react-router-dom": "6.4.2",
    "referrer-policy": "1.2.0",
    "regression": "2.0.1",
    "serve-static": "1.15.0",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@cutting/devtools": "workspace:*",
    "@cutting/eslint-config": "workspace:*",
    "@cutting/tsconfig": "workspace:*",
    "@cutting/useful-types": "workspace:*",
    "@mdx-js/mdx": "2.1.4",
    "@mdx-js/react": "2.1.4",
    "@mdx-js/rollup": "2.1.4",
    "@testing-library/react": "13.4.0",
    "@types/compression": "1.7.2",
    "@types/csurf": "1.11.2",
    "@types/d3": "7.4.0",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-array": "3.0.3",
    "@types/d3-selection": "^3.0.3",
    "@types/express": "4.17.14",
    "@types/loadable__component": "5.13.4",
    "@types/loadable__server": "5.12.6",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "6.1.5",
    "@types/react-router-dom": "5.3.3",
    "@types/regression": "2.0.2",
    "@types/url-join": "4.0.1",
    "@vanilla-extract/vite-plugin": "3.6.1",
    "@vitejs/plugin-react": "2.1.0",
    "copyfiles": "2.4.1",
    "eslint": "8.24.0",
    "install": "0.13.0",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "vite": "3.1.6",
    "vite-plugin-commonjs": "0.5.2",
    "vite-plugin-environment": "1.1.2",
    "vite-plugin-ssr": "0.4.39",
    "vite-plugin-svgr": "2.2.1",
    "vite-tsconfig-paths": "3.5.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18.2.0",
        "vite": "3.1.6"
      }
    }
  }
}
