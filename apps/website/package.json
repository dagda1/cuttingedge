    "@types/react-dom": "19.0.4",
    "@types/react-helmet": "6.1.11",
    "@types/react-is": "^19.0.0",
    "@types/react-router-dom": "5.3.3",
    "@types/regression": "2.0.6",
    "@types/url-join": "4.0.3",
    "@vanilla-extract/vite-plugin": "5.0.1",
    "copyfiles": "2.4.1",
    "install": "0.13.0",
