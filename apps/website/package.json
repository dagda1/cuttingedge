    "@mdx-js/react": "3.1.0",
    "@mdx-js/rollup": "3.1.0",
    "@testing-library/react": "16.2.0",
    "@types/compression": "1.7.5",
    "@types/csurf": "1.11.5",
    "@types/d3": "7.4.3",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-array": "3.2.1",
    "@types/d3-selection": "^3.0.11",
    "@types/express": "5.0.0",
    "@types/node": "22.13.9",
