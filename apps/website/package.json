{
  "name": "@cutting/website",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dagda1/cuttingedge.git"
  },
  "version": "4.29.20",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && rimraf dist && tsc -p ./tsconfig.dist.json && pnpm build:client && pnpm build:server && pnpm copy-files && ./fix-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "clean": "rimraf dist/ && mkdir dist",
    "copy-files": "copyfiles static/* dist/assets && copyfiles index.html dist && copyfiles -f dist/client/assets/* dist/assets copyfiles node_modules/@cutting/component-library/dist/esm/src.css* dist/assets",
    "start": "PORT=3000 nodemon --watch server.mts --watch src/server --exec 'ts-node --esm server.mts'",
    "lint": "eslint ./**/*.{ts,tsx} --fix",
    "serve": "NODE_ENV=production node dist/server.mjs",
    "deploy:web": "rm -rf ./pruned && NODE_ENV=production pnpm build && pnpm --filter @cutting/website --prod deploy pruned && ./bin/deploy.sh",
    "test": "NODE_ENV=test vitest"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.cs.ts"
  ],
  "dependencies": {
    "@cutting/component-library": "workspace:*",
    "@cutting/hooks": "workspace:*",
    "@cutting/react-hook-form-components": "workspace:*",
    "@cutting/svg": "workspace:*",
    "@cutting/use-get-parent-size": "workspace:*",
    "@cutting/use-mathjax": "workspace:*",
    "@cutting/util": "workspace:*",
    "@loadable/component": "5.15.3",
    "@vanilla-extract/css": "1.11.0",
    "@vanilla-extract/sprinkles": "1.6.0",
    "@visx/axis": "3.1.0",
    "@visx/curve": "3.0.0",
    "@visx/group": "3.0.0",
    "@visx/scale": "^3.0.0",
    "@visx/shape": "3.0.0",
    "@visx/text": "3.0.0",
    "@szhsin/react-menu": "^3.5.2",
    "assert-ts": "0.3.4",
    "classnames": "2.3.2",
    "cookie-parser": "1.4.6",
    "compression": "1.7.4",
    "core-js": "3.30.1",
    "csurf": "1.11.0",
    "d3-array": "3.2.3",
    "d3-scale": "4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "dayjs": "1.11.7",
    "express": "4.18.2",
    "global": "4.4.0",
    "helmet": "6.1.5",
    "history": "5.3.0",
    "immer": "9.0.21",
    "mathjs": "11.8.0",
    "nocache": "3.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.43.9",
    "react-router": "6.10.0",
    "react-router-dom": "6.10.0",
    "referrer-policy": "1.2.0",
    "regression": "2.0.1",
    "serve-static": "1.15.0",
    "ts-pattern": "4.2.2",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@cutting/devtools": "workspace:*",
    "@cutting/eslint-config": "workspace:*",
    "@cutting/tsconfig": "workspace:*",
    "@cutting/useful-types": "workspace:*",
    "@mdx-js/mdx": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@mdx-js/rollup": "2.3.0",
    "@testing-library/react": "14.0.0",
    "@types/compression": "1.7.2",
    "@types/csurf": "1.11.2",
    "@types/d3": "7.4.0",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-array": "3.0.4",
    "@types/d3-selection": "^3.0.5",
    "@types/express": "4.17.17",
    "@types/loadable__component": "5.13.4",
    "@types/loadable__server": "5.12.6",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/react-helmet": "6.1.6",
    "@types/react-router-dom": "5.3.3",
    "@types/regression": "2.0.2",
    "@types/url-join": "4.0.1",
    "@vanilla-extract/vite-plugin": "3.8.0",
    "@vitejs/plugin-react": "3.1.0",
    "copyfiles": "2.4.1",
    "eslint": "8.38.0",
    "install": "0.13.0",
    "nodemon": "2.0.22",
    "rimraf": "5.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vite-plugin-commonjs": "0.6.2",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-ssr": "0.4.113",
    "vite-plugin-svgr": "2.4.0",
    "vite-tsconfig-paths": "4.2.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18.2.0",
        "vite": "4.2.1"
      }
    }
  }
}
