    "immer": "10.1.1",
    "mathjs": "14.3.0",
    "nocache": "4.0.0",
    "puppeteer": "^24.3.1",
    "rc-slider": "^11.1.8",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.54.2",
    "react-is": "^19.0.0",
    "react-router": "7.2.0",
    "react-router-dom": "7.2.0",
    "referrer-policy": "1.2.0",
    "regression": "2.0.1",
    "serve-static": "1.16.2",
    "ts-pattern": "5.6.2",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@cutting/devtools": "workspace:*",
