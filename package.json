{
  "name": "cuttingedge",
  "version": "3.5.0",
  "private": true,
  "scripts": {
    "build": "cutting each-pkg yarn build",
    "build:libs": "cutting each-pkg -l yarn build",
    "test": "CI=true cutting each-pkg yarn test",
    "start": "yarn build && yarn --cwd packages/website/ ssr-start",
    "publish:all": "NODE_ENV=production cutting each-pkg yarn publish -- --non-interactive --verbose",
    "audit-packages": "cutting each-pkg cutting audit",
    "lint": "eslint 'packages/**/*.{ts,tsx,js}' -c ./packages/component-library/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "lint:js": "eslint --ext .js . -c ./packages/devtools/.eslintrc.json --ignore-path ./.eslintignore --fix"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "dayjs": "^1.8.26",
    "global": "^4.4.0",
    "history": "^4.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "http-proxy": "1.18.1",
    "husky": "^4.2.5",
    "inquirer": "^7.1.0",
    "node-sass": "^4.14.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "exec < /dev/tty; version-bump"
    }
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "resolutions": {
    "core-js": "3.6.5",
    "http-proxy": "1.18.1",
    "fork-ts-checker-webpack-plugin": "3.1.1"
  }
}
