{
  "name": "cuttingedge",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "build": "cutting each-pkg yarn build",
    "build:libs": "cutting each-pkg -l yarn build",
    "test": "CI=true cutting each-pkg yarn test",
    "start": "yarn build && yarn --cwd packages/website/ ssr-start",
    "publish:all": "NODE_ENV=production cutting each-pkg yarn publish -- --non-interactive --verbose",
    "audit-packages": "cutting each-pkg cutting audit",
    "lint": "eslint 'packages/**/*.{ts,tsx,js}' -c ./packages/component-library/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "lint:js": "eslint --ext .js . -c ./packages/devtools/.eslintrc.json --ignore-path ./.eslintignore --fix"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "dayjs": "^1.8.34",
    "global": "^4.4.0",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "http-proxy": "1.18.1",
    "husky": "^4.2.5",
    "inquirer": "^7.3.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass": "^4.14.1",
    "react-dev-tools": "^0.0.1",
    "typescript": "^4.1.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "resolutions": {
    "node-forge": "^0.10.0",
    "terser-webpack-plugin": "4.2.3"
  }
}