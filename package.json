{
  "name": "cuttingedge",
  "version": "4.22.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/devtools",
      "packages/testing",
      "packages/eslint-config",
      "packages/tsconfig",
      "packages/useful-types",
      "packages/devtools",
      "packages/util",
      "packages/hooks",
      "packages/use-get-parent-size",
      "packages/react-abortable-fetch",
      "packages/component-library",
      "packages/svg",
      "packages/use-mathjax",
      "packages/use-shortcuts",
      "packages/graphql-explorer",
      "packages/website"
    ]
  },
  "scripts": {
    "audit-packages": "cutting each-pkg cutting audit",
    "build": "yarn nx run-many --target=build --all --parallel",
    "build:tsc": "tsc -b ./tsconfig.packages.json",
    "clean": "yarn build:tsc -- --clean && find . \\( -name node_modules -o -name dir2 -o -path name \\) -prune -false -o -name 'dist' -type d   -exec rm -rf {} +",
    "lint": "eslint 'packages/**/*.{ts,tsx,js}' -c ./packages/component-library/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "lint:js": "eslint --ext .js . -c ./packages/devtools/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "start": "yarn build && yarn --cwd packages/website start",
    "test": "CI=true yarn workspaces foreach -vi run test",
    "watch": "yarn build:tsc && yarn tsc -b ./tsconfig.packages.json --watch",
    "version": "changeset version && yarn",
    "release": "yarn build && changeset publish"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@capsizecss/core": "^3.0.0",
    "@capsizecss/vanilla-extract": "^1.0.0",
    "@changesets/cli": "^2.17.0",
    "@cutting/useful-types": "4.25.2",
    "@nrwl/cli": "latest",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "latest",
    "@nrwl/workspace": "latest",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@vanilla-extract/babel-plugin": "^1.1.2",
    "@vanilla-extract/css": "^1.6.3",
    "@vanilla-extract/esbuild-plugin": "^2.0.0",
    "@vanilla-extract/sprinkles": "^1.3.1",
    "@vanilla-extract/webpack-plugin": "^2.1.0",
    "css-what": "^5.1.0",
    "csstype": "^3.0.8",
    "dedent": "^0.7.0",
    "deepmerge": "^4.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "gradient-parser": "^1.0.2",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "normalize-url": "^7.0.2",
    "polished": "^4.1.3",
    "postcss-loader": "^6.2.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.4.1",
    "ssri": "8.0.1",
    "tailwindcss": "^2.2.7",
    "trim-newlines": "^4.0.2",
    "typescript": "^4.4.4",
    "url-parse": "^1.5.3"
  },
  "packageManager": "yarn@3.1.0",
  "volta": {
    "node": "14.18.1"
  },
  "resolutions": {
    "@types/react": "^17.0.0",
    "@types/webpack": "^5.28.0",
    "browserslist": "^4.16.5",
    "commander": "^8.1.0",
    "d3-array": "^2.4.0",
    "d3-scale": "^3.3.0",
    "d3-time": "^2.1.1",
    "effection": "2.0.0-beta.22",
    "glob-parent": "^6.0.2",
    "normalize-url": "^7.0.2",
    "prettier": "^2.4.1",
    "url-parse": "^1.5.3",
    "webpack": "^5.61.0"
  }
}
