{
  "name": "cuttingedge",
  "version": "4.22.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "audit-packages": "cutting each-pkg cutting audit",
    "build": "turbo run build --scope=\"packages/**/*\"",
    "build:apps": "turbo run build --scope=\"@cutting/website\"",
    "build:tsc": "tsc -b ./tsconfig.packages.json",
    "clean": "pnpm build:tsc -- --clean && find . \\( -name node_modules -o -name dir2 -o -path name \\) -prune -false -o -name 'dist' -type d   -exec rm -rf {} +",
    "lint": "turbo run lint",
    "lint:js": "eslint --ext .js . -c ./packages/devtools/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "prepare": "husky install",
    "test": "CI=true turbo run test",
    "watch": "build:tsc && pnpm tsc -b ./tsconfig.packages.json --watch",
    "version": "changeset version && pnpm install --lockfile-only",
    "release": "pnpm build && changeset publish"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "core-js": "^3.21.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@changesets/cli": "^2.20.0",
    "@cutting/devtools": "workspace:^4.31.6",
    "@cutting/tsconfig": "workspace:^4.26.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "file-loader": "^6.2.0",
    "follow-redirects": "^1.14.8",
    "html-loader": "^3.1.0",
    "husky": "^7.0.4",
    "markdown-loader": "^8.0.0",
    "lint-prepush": "^2.2.0",
    "node-fetch": "^3.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "turbo": "^1.1.2",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-prepush": {
    "base": "main",
    "tasks": {
      "*.{js,jsx,ts,tsx,js,md}": [
        "pnpm lint"
      ]
    }
  },
  "packageManager": "pnpm@6.30.1",
  "volta": {
    "node": "16.13.2",
    "pnpm": "6.31.0"
  },
  "pnpm": {
    "overrides": {
      "@babel/types@<7.17.0": ">=7.17.3",
      "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
      "nth-check@<2.0.1": ">=2.0.1",
      "immer@<9.0.6": ">=9.0.6",
      "tar@<4.4.18": ">=4.4.18",
      "browserslist@>=4.0.0 <4.16.5": ">=4.16.5",
      "trim@<0.0.3": ">=0.0.3",
      "postcss@<8.2.13": ">=8.2.13",
      "node-forge@<1.0.0": ">=1.0.0",
      "marked@<4.0.10": ">=4.0.10",
      "node-fetch@<2.6.7": ">=2.6.7",
      "glob-parent@<5.1.2": ">=5.1.2",
      "follow-redirects@<1.14.8": ">=1.14.8",
      "strip-ansi@>=3.0.1": "=3.0.1"
    }
  }
}
