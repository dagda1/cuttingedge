{
  "name": "cuttingedge",
  "version": "4.22.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "audit-packages": "devtools each-pkg devtools audit",
    "all": "pnpm i && pnpm build && pnpm lint && pnpm test",
    "build": "turbo run build",
    "build:tsc": "tsc -b ./tsconfig.packages.json",
    "clean": "rm -rf ./packages/*/{tsconfig.*tsbuildinfo,dist}",
    "lint": "turbo run lint",
    "lint:js": "eslint --ext .js . -c ./packages/devtools/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "prepare": "husky install",
    "test": "CI=true turbo run test -- --coverage --run",
    "watch": "pnpm build:tsc && pnpm tsc -b ./tsconfig.packages.json --watch",
    "ci:version": "changeset version && pnpm install --frozen-lockfile false",
    "prerelease": "pnpm clean && pnpm install && pnpm run build",
    "release": "pnpm run prerelease && changeset publish && git push --follow-tags && git status && pnpm -r publish"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "7.18.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/runtime": "7.19.0",
    "@changesets/cli": "2.24.4",
    "@cutting/devtools": "workspace:*",
    "@cutting/tsconfig": "^4.38.0",
    "@testing-library/jest-dom": "5.16.5",
    "@types/node": "18.7.18",
    "@vitest/coverage-c8": "0.23.4",
    "@vitejs/plugin-react": "2.1.0",
    "@vanilla-extract/vite-plugin": "3.5.0",
    "babel-loader": "8.2.5",
    "core-js": "3.25.1",
    "css-loader": "6.7.1",
    "eslint": "8.23.1",
    "file-loader": "6.2.0",
    "follow-redirects": "1.15.2",
    "html-loader": "4.1.0",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "markdown-loader": "8.0.0",
    "node-fetch": "3.2.10",
    "regenerator-runtime": "0.13.9",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.1",
    "turbo": "1.4.6",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "vitest": "0.23.4",
    "webpack": "5.74.0",
    "whatwg-fetch": "3.6.2"
  },
  "packageManager": "pnpm@7.11.0",
  "volta": {
    "node": "16.17.0",
    "npm": "8.19.2"
  },
  "lint-staged": {
    "{packages|apps}/**/*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix"
    ]
  },
  "pnpm": {
    "overrides": {
      "@babel/core@<7.19.1": ">=7.19.1",
      "@babel/types@<7.19.0": ">=7.19.3",
      "@babel/runtime@<7.19.0": ">=7.19.0",
      "ansi-regex@>2.1.1 <5.0.1": "=5.0.1",
      "acorn@<8.8.0": ">=8.8.0",
      "commander@<9.0.0": ">=9.4.0",
      "nth-check@<2.0.1": ">=2.1.1",
      "immer@<9.0.6": ">=9.0.15",
      "tar@<6.1.11": ">=6.1.11",
      "browserslist@>=4.0.0 <4.21.4": ">=4.21.4",
      "trim@<1.0.1": ">=1.0.1",
      "postcss@<8.4.16": ">=8.4.16",
      "node-forge@<1.3.1": ">=1.3.1",
      "marked@<4.1.0": ">=4.1.0",
      "node-fetch@<3.2.10": ">=3.2.10",
      "glob-parent@<6.0.2": ">=6.0.2",
      "follow-redirects@<1.15.1": ">=1.15.1"
    }
  }
}
