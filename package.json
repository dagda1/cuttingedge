{
  "name": "cuttingedge",
  "version": "4.4.0",
  "private": true,
  "scripts": {
    "build": "yarn workspaces run build",
    "test": "CI=true yarn workspaces run test",
    "start": "yarn build && yarn --cwd packages/website start",
    "publish:all": "NODE_ENV=production cutting each-pkg yarn publish -- --non-interactive --verbose",
    "audit-packages": "cutting each-pkg cutting audit",
    "lint": "eslint 'packages/**/*.{ts,tsx,js}' -c ./packages/component-library/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "lint:js": "eslint --ext .js . -c ./packages/devtools/.eslintrc.json --ignore-path ./.eslintignore --fix"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "dayjs": "^1.9.7",
    "global": "^4.4.0",
    "history": "^4.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "http-proxy": "1.18.1",
    "husky": "^4.3.6",
    "inquirer": "^7.3.3",
    "node-sass": "^5.0.0",
    "react-dev-tools": "^0.0.1",
    "typescript": "^4.1.3"
  },
  "workspaces": {
    "packages": [
      "packages/devtools",
      "packages/testing",
      "packages/eslint-config",
      "packages/useful-types",
      "packages/util",
      "packages/hooks",
      "packages/use-abort",
      "packages/component-library",
      "packages/svg",
      "packages/use-shortcuts",
      "packages/website"
    ]
  },
  "resolutions": {
    "@types/react": "^17.0.0",
    "@types/webpack": "4.41.21",
    "d3-array": "^2.4.0",
    "node-forge": "^0.10.0"
  }
}