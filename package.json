{
  "name": "cuttingedge",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "build": "cutting each-pkg yarn build",
    "build:libs": "cutting each-pkg -l yarn build",
    "test": "CI=true cutting each-pkg yarn test",
    "start": "yarn build && yarn --cwd packages/website/ ssr-start",
    "publish:all": "NODE_ENV=production cutting each-pkg yarn publish -- --non-interactive --verbose",
    "audit-packages": "cutting each-pkg cutting audit",
    "lint": "eslint 'packages/**/*.{ts,tsx,js}' -c ./packages/component-library/.eslintrc.json --ignore-path ./.eslintignore --fix",
    "lint:js": "eslint --ext .js . -c ./packages/devtools/.eslintrc.json --ignore-path ./.eslintignore --fix"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "dayjs": "^1.9.7",
    "global": "^4.4.0",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "^1.16.0",
    "http-proxy": "1.18.1",
    "husky": "^4.3.6",
    "inquirer": "^7.3.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass": "^5.0.0",
    "react-dev-tools": "^0.0.1",
    "typescript": "^4.1.3"
  },
  "workspaces": {
    "packages": [
      "packages/devtools",
      "packages/eslint-config",
      "packages/useful-types",
      "packages/util",
      "packages/hooks",
      "packages/component-library",
      "packages/use-shortcuts",
      "packages/graphql-explorer",
      "packages/website"
    ]
  },
  "resolutions": {
    "node-forge": "^0.10.0",
    "postcss": "^8.2.1",
    "terser-webpack-plugin": "4.2.3"
  }
}